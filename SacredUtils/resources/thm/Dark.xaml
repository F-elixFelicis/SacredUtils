<Globalization:StyleResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Globalization="clr-namespace:WPFSharp.Globalizer;assembly=WPFSharp.Globalizer"
    Name="Dark">

    <SolidColorBrush x:Key="BorderBorderBrush" Color="#323232" />
    <SolidColorBrush x:Key="LabelForegroundBrush" Color="#FF4285F4" />
    <SolidColorBrush x:Key="ComboBoxBorderBrush" Color="#535353" />
    <SolidColorBrush x:Key="ComboBoxForegroundBrush" Color="#FFB4B4B4" />
    <SolidColorBrush x:Key="LabelWarningForegroundBrush" Color="#FFF44242" />
    <SolidColorBrush x:Key="BackgroundColorBrush" Color="#212121" />
    <SolidColorBrush x:Key="BigBallColorBrush" Color="#323133" />
    <SolidColorBrush x:Key="SmallBallColorBrush" Color="#3C3B3D" />
    <SolidColorBrush x:Key="CategoryForegroundColorBrush" Color="#FF737373" />
    <SolidColorBrush x:Key="CategoryNotActiveColorBrush" Color="#212121" />
    <SolidColorBrush x:Key="CategoryActiveColorBrush" Color="#323232" />
    <Color
        x:Key="SliderLineColorBrush"
        A="255"
        B="83"
        G="83"
        R="83" />

</Globalization:StyleResourceDictionary>