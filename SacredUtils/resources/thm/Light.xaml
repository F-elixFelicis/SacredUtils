<Globalization:StyleResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Globalization="clr-namespace:WPFSharp.Globalizer;assembly=WPFSharp.Globalizer"
    Name="Light">

    <SolidColorBrush x:Key="BorderBorderBrush" Color="#E6E6E6" />
    <SolidColorBrush x:Key="LabelForegroundBrush" Color="#FF4285F4" />
    <SolidColorBrush x:Key="ComboBoxBorderBrush" Color="#C3C3C3" />
    <SolidColorBrush x:Key="ComboBoxForegroundBrush" Color="#FF444444" />
    <SolidColorBrush x:Key="LabelWarningForegroundBrush" Color="#FFF44242" />
    <SolidColorBrush x:Key="BackgroundColorBrush" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="BigBallColorBrush" Color="#FF96CAFF" />
    <SolidColorBrush x:Key="SmallBallColorBrush" Color="#FFBFDEFF" />
    <SolidColorBrush x:Key="CategoryForegroundColorBrush" Color="#FF737373" />
    <SolidColorBrush x:Key="CategoryNotActiveColorBrush" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="CategoryActiveColorBrush" Color="#E6E6E6" />
    <Color
        x:Key="SliderLineColorBrush"
        A="255"
        B="195"
        G="195"
        R="195" />

</Globalization:StyleResourceDictionary>