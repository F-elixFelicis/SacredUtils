                <Slider x:Name="VolumeSfxSlider" HorizontalAlignment="Left" VerticalAlignment="Top" Width="217" Margin="363,255,0,0" Style="{StaticResource MaterialDesignDiscreteSlider}" Maximum="128" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" ValueChanged="VolumeSfxSlider_ValueChanged"/>
                <Slider x:Name="VolumeVoiceSlider" HorizontalAlignment="Left" VerticalAlignment="Top" Width="217" Margin="363,300,0,0" Style="{StaticResource MaterialDesignDiscreteSlider}" Maximum="128" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" ValueChanged="VolumeVoiceSlider_ValueChanged"/>

                <ToggleButton x:Name="SoundToggleBtn" Style="{StaticResource MaterialDesignSwitchToggleButton}" Margin="528,152,12,367" Width="52" Height="25" Click="SoundToggleBtn_Click"/>
            </Grid>

