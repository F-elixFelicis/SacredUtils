<Window x:Class="SacredUtils.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        Title="SacredUtils" Height="752" Width="986" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" WindowStyle="None" AllowsTransparency="True" Background="{x:Null}" Icon="Resources/Images/icons8_Wet.ico" Loaded="Window_Loaded" KeyDown="Window_KeyDown">
    <Grid MouseDown="Grid_MouseDown">
        <Grid.RowDefinitions>
            <RowDefinition Height="24"/>
            <RowDefinition Height="364"/>
            <RowDefinition Height="363"/>
        </Grid.RowDefinitions>

        <Label x:Name="FlexibleSpaceElementColor" Foreground="White" Grid.Row="0" Visibility="Hidden"/>

        <Label x:Name="ToolSpaceElementColor" Foreground="#7986cb" Grid.Row="0" Visibility="Hidden"/>

        <Label x:Name="SettingsNameColor" Foreground="#DD323232" Grid.Row="0" Visibility="Hidden"/>

        <Label x:Name="CardCaptionsColor" Foreground="#DD404040" Grid.Row="0" Visibility="Hidden"/>

        <materialDesign:Card x:Name="CardBackgroundColor" Background="#FFFFFFFF" Grid.Row="0" Visibility="Hidden"/>

        <ComboBox Grid.Row="1" Visibility="Hidden" x:Name="ColorCombobox" Background="White" Foreground="Black"/>

        <TextBox Grid.Row="1" Visibility="Hidden" x:Name="ColorTxBox" Foreground="Black"/>

        <Border Grid.Row="0" x:Name="CornerBordersBorder" CornerRadius="9,9,0,0" Background="#FF303F9F"/>

        <StackPanel x:Name="StackPanelPrimary" Background="#303F9F" Grid.Row="0">
            <StackPanel.OpacityMask>
                <VisualBrush Visual="{Binding ElementName=CornerBordersBorder}" Stretch="Uniform"/>
            </StackPanel.OpacityMask>
        </StackPanel>
        <StackPanel x:Name="StackPanelMiddle" Background="#3F51B5" Grid.Row="1"/>
        <StackPanel x:Name="StackPanelLight" Background="#C5CAE9" Grid.Row="2"/>

        <Button x:Name="CloseBtn" Grid.Row="0" Height="25" Style="{StaticResource MaterialDesignToolForegroundButton}" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Margin="0,0,4,0" VerticalAlignment="Center" HorizontalAlignment="Right" Width="25" materialDesign:RippleAssist.RippleSizeMultiplier="0.7" materialDesign:RippleAssist.IsCentered="True" Click="CloseBtn_Click">
            <materialDesign:PackIcon Kind="Circle" Foreground="{Binding Foreground, ElementName=ToolSpaceElementColor}" Height="10" Width="10" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Button>

        <Button x:Name="MinimizeBtn" Grid.Row="0" Height="25" Style="{StaticResource MaterialDesignToolForegroundButton}" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Margin="0,0,26,0" VerticalAlignment="Center" HorizontalAlignment="Right" Width="25" materialDesign:RippleAssist.RippleSizeMultiplier="0.7" materialDesign:RippleAssist.IsCentered="True" Click="MinimizeBtn_Click">
            <materialDesign:PackIcon Kind="Circle" Foreground="{Binding Foreground, ElementName=ToolSpaceElementColor}" Height="10" Width="10" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Button>

        <Label Content="SacredUtils (Alpha 3)." Grid.Row="1" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans" FontSize="20" Foreground="{Binding Foreground, ElementName=FlexibleSpaceElementColor}" Margin="29,16,700,313" TextOptions.TextHintingMode="Animated"/>

        <Button x:Name="ChangeColor" Grid.Row="1" Height="50" Style="{StaticResource MaterialDesignToolForegroundButton}" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Margin="0,10,16,304" VerticalAlignment="Center" HorizontalAlignment="Right" Width="50" materialDesign:RippleAssist.RippleSizeMultiplier="0.7" materialDesign:RippleAssist.IsCentered="True" Click="ChangeColor_Click" >
            <materialDesign:PackIcon Kind="InvertColors" Foreground="{Binding Foreground, ElementName=FlexibleSpaceElementColor}" Height="25" Width="25" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Button>

        <Button x:Name="GitHub" Grid.Row="1" Height="50" Style="{StaticResource MaterialDesignToolForegroundButton}" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Margin="0,10,66,304" VerticalAlignment="Center" HorizontalAlignment="Right" Width="50" materialDesign:RippleAssist.RippleSizeMultiplier="0.7" materialDesign:RippleAssist.IsCentered="True" Click="GitHubBtn_Click">
            <materialDesign:PackIcon Kind="Git" Foreground="{Binding Foreground, ElementName=FlexibleSpaceElementColor}" Height="25" Width="25" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Button>

        <Grid x:Name="SettingsGrid" Row="1" RowSpan="2">
            <materialDesign:Card VerticalAlignment="Top" HorizontalAlignment="Left" Padding="300,156,99,-58" Margin="107,104,0,0" ScrollViewer.VerticalScrollBarVisibility="Disabled" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Width="162" Height="544" Background="{Binding Background, ElementName=CardBackgroundColor}" />
            <materialDesign:Card VerticalAlignment="Top" HorizontalAlignment="Right" Padding="300,156,99,-58" Margin="0,104,107,0" ScrollViewer.VerticalScrollBarVisibility="Disabled" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Width="592" Height="544" Background="{Binding Background, ElementName=CardBackgroundColor}"/>

            <Label Content="Выбор настроек." Foreground="{Binding Foreground, ElementName=CardCaptionsColor}" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" FontSize="14" Margin="119,118,740,582" Height="27"/>
            <Label Content="Настройки." Foreground="{Binding Foreground, ElementName=CardCaptionsColor}" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" FontSize="14" Margin="304,118,594,582" Height="27"/>

