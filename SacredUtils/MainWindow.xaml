            <ListBox x:Name="SettingsListBox" HorizontalAlignment="Left" Height="494" HorizontalContentAlignment="Left" VerticalAlignment="Top" Width="162" Margin="107,154,0,0" ScrollViewer.VerticalScrollBarVisibility="Disabled" VerticalContentAlignment="Stretch" Padding="0" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=CardCaptionsColor}" SelectionChanged="SettingsListBox_SelectionChanged">
                <ListBoxItem Content="  Настройки графики 1."/>
                <ListBoxItem Content="  Настройки графики 2."/>
                <ListBoxItem Content="  Настройки звука."/>
                <ListBoxItem Content="  Сетевые настройки 1."/>
                <ListBoxItem Content="  Сетевые настройки 2."/>
                <ListBoxItem Content="  Настройки чата."/>
                <ListBoxItem Content="  Игровые настройки 1."/>
                <ListBoxItem Content="  Игровые настройки 2."/>
                <ListBoxItem Content="  Настройка шрифтов."/>
                <ListBoxItem Content="  Другие настройки."/>
                <ListBoxItem Content="  Моддинг Sacred'а."/>
                <ListBoxItem Content="  Настройки SacredUtils."/>
                <ListBoxItem Content="  О SacredUtils."/>
                <Separator Margin="-8,0,-8,0"/>
                <ListBoxItem Content="  Запустить Sacred."/>
                <ListBoxItem Content="  Обновить настройки."/>
            </ListBox>

            <Grid x:Name="GraphicsSettings01Grid" Margin="287,104,107,79" Visibility="Hidden">
                <Label Content="128Mbit памяти видеокарте." HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=SettingsNameColor}" Margin="15,107,0,0" Height="25"/>
                <Label Content="Сглаживание изображения." HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=SettingsNameColor}" Margin="15,152,0,0" Height="25"/>
                <Label Content="Режим видео совместимости." HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=SettingsNameColor}" Margin="15,197,0,0" Height="25"/>
                <Label Content="Глубина цвета 32 bit." HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=SettingsNameColor}" Margin="15,242,0,0" Height="25"/>
                <Label Content="Вертикальная синхронизация." HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=SettingsNameColor}" Margin="15,287,0,0" Height="25"/>
                <Label Content="Полноэкранный режим." HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=SettingsNameColor}" Margin="15,377,0,0" Height="25"/>
                <Label Content="Принудительно черные тени." HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=SettingsNameColor}" Margin="15,332,0,0" Height="25"/>
                <Label Content="Показывать игровые видео." HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=SettingsNameColor}" Margin="15,422,0,0" Height="25"/>

                <Separator HorizontalAlignment="Left" Height="10" Margin="0,92,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>
                <Separator HorizontalAlignment="Left" Height="10" Margin="0,137,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>
                <Separator HorizontalAlignment="Left" Height="10" Margin="0,182,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>
                <Separator HorizontalAlignment="Left" Height="10" Margin="0,227,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>
                <Separator HorizontalAlignment="Left" Height="10" Margin="0,272,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>
                <Separator HorizontalAlignment="Left" Height="10" Margin="0,317,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>
                <Separator HorizontalAlignment="Left" Height="10" Margin="0,362,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>
                <Separator HorizontalAlignment="Left" Height="10" Margin="0,407,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>
                <Separator HorizontalAlignment="Left" Height="10" Margin="0,452,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>

                <ToggleButton x:Name="FsaaFilterToggleBtn" Style="{StaticResource MaterialDesignSwitchToggleButton}" Margin="525,152,15,367" Height="25" Click="FsaaFilterToggleBtn_Click" Width="52"/>
                <ToggleButton x:Name="GfxLimit128ToggleButton" Style="{StaticResource MaterialDesignSwitchToggleButton}" Margin="525,107,15,412" Height="25" Click="GfxLimit128ToggleButton_Click"/>
                <ToggleButton x:Name="CompatVideoToggleBtn" Style="{StaticResource MaterialDesignSwitchToggleButton}" Margin="525,197,15,322" Height="25" Click="CompatVideoToggleBtn_Click" Width="52"/>
                <ToggleButton x:Name="Gfx32ToggleBtn" Style="{StaticResource MaterialDesignSwitchToggleButton}" Margin="525,242,15,277" Height="25" Click="Gfx32ToggleBtn_Click" Width="52"/>
