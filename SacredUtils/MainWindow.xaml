                <ToggleButton x:Name="FullscreenToggleBtn" Style="{StaticResource MaterialDesignSwitchToggleButton}" Margin="525,377,15,142" Height="25" Click="FullscreenToggleBtn_Click" Width="52"/>
                <ToggleButton x:Name="ShowMovieToggleBtn" Style="{StaticResource MaterialDesignSwitchToggleButton}" Margin="525,422,15,97" Height="25" Width="52" Click="ShowMovieToggleBtn_Click"/>
            </Grid>

            <Grid x:Name="GraphicsSettings02Grid" Margin="287,104,107,79" Width="592" Height="544" Visibility="Hidden">
                <Label Content="Качество графики." HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=SettingsNameColor}" Margin="15,152,0,0" Height="25"/>
                <Label Content="Прозрачность карты." HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=SettingsNameColor}" Margin="15,197,0,0" Width="144" Height="25"/>
                <Label Content="Темнота ночи." HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=SettingsNameColor}" Margin="15,242,0,0" Height="25"/>
                <Label Content="Картинка при запуске игры." HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=SettingsNameColor}" Margin="15,287,0,0" Height="25"/>
                <Label Content="Картинка при save \ load игры." HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=SettingsNameColor}" Margin="15,332,0,0" Height="25"/>
                <Label Content="Язык интерфейса." HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=SettingsNameColor}" Margin="15,377,0,0" Height="25"/>

                <Separator HorizontalAlignment="Left" Height="10" Margin="0,137,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>
                <Separator HorizontalAlignment="Left" Height="10" Margin="0,182,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>
                <Separator HorizontalAlignment="Left" Height="10" Margin="0,227,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>
                <Separator HorizontalAlignment="Left" Height="10" Margin="0,272,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>
                <Separator HorizontalAlignment="Left" Height="10" Margin="0,317,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>
                <Separator HorizontalAlignment="Left" Height="10" Margin="0,362,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>
                <Separator HorizontalAlignment="Left" Height="10" Margin="0,407,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>

                <ComboBox x:Name="DetailLevelCmbBox" VerticalAlignment="Top" Margin="369,150,21,0" IsSynchronizedWithCurrentItem="False" UseLayoutRounding="False" Background="{Binding Background, ElementName=ColorCombobox}" IsReadOnly="True" Height="27" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans" SelectedIndex="2" Width="203" DropDownClosed="DetailLevelCmbBox_DropDownClosed" Foreground="{Binding Foreground, ElementName=ColorCombobox}">
                    <ComboBoxItem Content="Низкое"/>
                    <ComboBoxItem Content="Среднее"/>
                    <ComboBoxItem Content="Высокое"/>
                </ComboBox>
                <ComboBox x:Name="InterfaceLanguageCmbBox" HorizontalAlignment="Left" VerticalAlignment="Top" Width="211" Margin="369,375,0,0" IsSynchronizedWithCurrentItem="False" UseLayoutRounding="False" Background="{Binding Background, ElementName=ColorCombobox}" IsReadOnly="True" Height="27" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans" SelectedIndex="0" DropDownClosed="InterfaceLanguageCmbBox_DropDownClosed" Foreground="{Binding Foreground, ElementName=ColorCombobox}">
                    <ComboBoxItem Content="Русский"/>
                    <ComboBoxItem Content="Английский"/>
                    <ComboBoxItem Content="Немецкий"/>
                    <ComboBoxItem Content="Испанский"/>
                    <ComboBoxItem Content="Французский"/>
                </ComboBox>

                <Slider x:Name="MinimapAlphaSlider" HorizontalAlignment="Left" VerticalAlignment="Top" Width="217" Margin="363,165,0,0" Style="{StaticResource MaterialDesignDiscreteSlider}" Maximum="255" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" ValueChanged="MinimapAlphaSlider_ValueChanged"/>
                <Slider x:Name="NightDarknessSlider" HorizontalAlignment="Left" VerticalAlignment="Top" Width="217" Margin="363,209,0,0" Style="{StaticResource MaterialDesignDiscreteSlider}" Maximum="100" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" ValueChanged="NightDarknessSlider_ValueChanged"/>

                <TextBox x:Name="GfxStartupTxBox" MaxLength="260" HorizontalAlignment="Left" Height="27" TextWrapping="Wrap" VerticalAlignment="Top" Width="203" Margin="369,286,0,0" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans" KeyUp="GfxStartupTxBox_KeyUp" Foreground="{Binding Foreground, ElementName=ColorTxBox}"/>
                <TextBox x:Name="GfxLoadingTxBox" MaxLength="260" Foreground="{Binding Foreground, ElementName=ColorTxBox}" HorizontalAlignment="Left" Height="27" TextWrapping="Wrap" VerticalAlignment="Top" Width="203" Margin="369,331,0,0" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans" KeyUp="GfxLoadingTxBox_KeyUp"/>
            </Grid>

            <Grid x:Name="SoundSettingsGrid" Margin="287,104,107,79" Width="592" Height="544" Visibility="Hidden">
                <Label Content="Качество звука." HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=SettingsNameColor}" Margin="15,197,0,0" Height="25"/>
                <Label Content="Громкость музыки." HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=SettingsNameColor}" Margin="15,242,0,0" Width="144" Height="25"/>
                <Label Content="Громкость эффектов." HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=SettingsNameColor}" Margin="15,287,0,0" Height="25"/>
                <Label Content="Громкость голоса." HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=SettingsNameColor}" Margin="15,332,0,0" Height="25"/>
                <Label Content="Включить звук в игре." HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=SettingsNameColor}" Margin="15,152,0,0" Height="25"/>
                <Label Content="Язык озвучки." HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans Medium" Foreground="{Binding Foreground, ElementName=SettingsNameColor}" Margin="15,377,0,0" Height="25"/>

                <Separator HorizontalAlignment="Left" Height="10" Margin="0,137,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>
                <Separator HorizontalAlignment="Left" Height="10" Margin="0,182,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>
                <Separator HorizontalAlignment="Left" Height="10" Margin="0,227,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>
                <Separator HorizontalAlignment="Left" Height="10" Margin="0,272,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>
                <Separator HorizontalAlignment="Left" Height="10" Margin="0,317,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>
                <Separator HorizontalAlignment="Left" Height="10" Margin="0,362,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>
                <Separator HorizontalAlignment="Left" Height="10" Margin="0,407,0,0" VerticalAlignment="Top" Width="592" IsEnabled="False"/>

                <ComboBox x:Name="SoundQualityCmbBox" VerticalAlignment="Top" Margin="371,197,19,0" IsSynchronizedWithCurrentItem="False" UseLayoutRounding="False" Background="{Binding Background, ElementName=ColorCombobox}" IsReadOnly="True" Height="27" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans" SelectedIndex="2" DropDownClosed="SoundQualityCmbBox_DropDownClosed" Foreground="{Binding Foreground, ElementName=ColorCombobox}">
                    <ComboBoxItem Content="Низкое"/>
                    <ComboBoxItem Content="Среднее"/>
                    <ComboBoxItem Content="Высокое"/>
                </ComboBox>

                <ComboBox x:Name="SoundLanguageCmbBox" VerticalAlignment="Top" Margin="371,375,19,0" IsSynchronizedWithCurrentItem="False" UseLayoutRounding="False" Background="{Binding Background, ElementName=ColorCombobox}" IsReadOnly="True" Height="27" FontFamily="/SacredUtils;component/Resources/Fonts/#Google Sans" SelectedIndex="0" Foreground="{Binding Foreground, ElementName=ColorCombobox}" DropDownClosed="SoundLanguageCmbBox_DropDownClosed">
                    <ComboBoxItem Content="Русский"/>
                    <ComboBoxItem Content="Английский"/>
                    <ComboBoxItem Content="Немецкий"/>
                    <ComboBoxItem Content="Испанский"/>
                </ComboBox>

